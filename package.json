{
  "name": "web-01",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "build-vercel": "turbo run build-vercel",
    "generate": "pnpm --dir packages/generator plop"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest",
    "vercel": "^28.18.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.30.0"
}
